<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 


<hibernate-mapping>
    <class name="cat.copernic.m03uf06review.orm.Cotxes" table="cotxe">
        <id name="Model" type="string" column="Model"> <!--Clau primaria de la taula SQL-->
            <generator class="native"/>
        </id>
        <property name="Potencia_motor" column="Potencia_motor" type="java.lang.Integer"/>
        <property name="Acceleracio_motor" column="Acceleracio_cent" type="float"/>
        <property name="Origen_europeu" column="Origen_europeu" type="boolean"/>
        <property name="Data_fabricacio" column="Data_fabricacio" type="date"/> <!--El date va en minuscula-->
    </class>
</hibernate-mapping>
